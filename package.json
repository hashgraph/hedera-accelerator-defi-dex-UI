  "license": "MIT",
  "main": "./src/index.tsx",
  "dependencies": {
    "@chakra-ui/icons": "^2.0.4",
    "@chakra-ui/react": "^2.2.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@material-ui/core": "^4.12.4",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-query-devtools": "^4.33.0",
    "@testing-library/jest-dom": "^6.1.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/numeral": "^2.0.2",
    "@types/quill": "^2.0.9",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dropzone": "^5.1.0",
    "axios": "^1.4.0",
    "chakra-ui-steps": "^2.0.4",
    "ethers": "^5.7.2",
    "framer-motion": "^10.16.1",
    "hashconnect": "^0.2.4",
    "numeral": "^2.0.6",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.4",
    "react-paginate": "^8.1.4",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "react-window": "^1.8.9",
    "ts-debounce": "^4.0.0",
    "web-vitals": "^3.4.0",
    "web3": "^4.1.1",
    "zustand": "^4.1.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "test:coverage": "vitest run --coverage",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006 -s public",
    "build:storybook": "storybook build -o docs",
    "deploy:storybook": "gh-pages -d docs",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --fix .js,.jsx,.ts,.tsx",
    "format": "npx prettier -c src",
    "format:write": "npx prettier -c src --write",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "eslintConfig": {
    "root": true,
    "plugins": [
      "react",
      "react-hooks"
    ],
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "max-len": [
        "error",
        {
          "code": 120
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
          "eslint-plugin-tsdoc"
        ],
        "rules": {
          "tsdoc/syntax": "warn",
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/ban-ts-comment": "off"
        }
      },
      {
        "files": [
          "**/__tests__/**/*.[jt]s?(x)', '**/?(*.)+(spec|test).[jt]s?(x)"
        ],
        "plugins": [
          "testing-library"
        ],
        "extends": [
          "plugin:testing-library/react"
        ]
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-docs": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-mdx-gfm": "^7.3.2",
    "@storybook/node-logger": "^7.3.2",
    "@storybook/preset-create-react-app": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/react-webpack5": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.3.2",
    "@testing-library/dom": "^9.3.1",
    "@types/node": "^20.5.4",
    "@types/ramda": "^0.29.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "c8": "^8.0.1",
    "craco": "^0.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "^6.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.0",
    "jsdom": "^22.0.0",
    "lint-staged": "^14.0.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.3.2",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2",
    "webpack": "^5.76.0"
  }
}
